{
  "name": "H Media Scraper",
  "version": "0.2",
  "description": "H Media Scraper",
  "manifest_version": 2,
  "permissions": [
    "webRequest","webRequestBlocking", "tabs", "https://*/*", "http://*/*"
  ],
  "background": {
    "scripts": ["bgp.js"],
    "matches": ["*://*"],
    "persistent": true
  }
}
